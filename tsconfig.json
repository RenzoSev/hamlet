{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "styles/*": ["app/styles/*"],
      "shared/*": ["app/styles/shared/*"],
      "store/*": ["app/store/*"],
      "routes/*": ["app/routes/*"],
      "hooks/*": ["app/hooks/*"],
      "pages/*": ["app/pages/*"],
      "components/*": ["app/components/*"],
      "types/*": ["app/types/*"],
      "ophelia/*": ["ophelia/*"]
    }
  }
}
